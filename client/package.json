{
    "name": "template-react-ts",
    "description": "A Phaser 3 TypeScript project template that demonstrates React communication and uses Vite for bundling.",
    "version": "1.0.2",
    "type": "module",
    "author": "zak3939",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-react-ts/issues"
    },
    "homepage": "https://github.com/phaserjs/template-react-ts#readme",
    "keywords": [
        "phaser",
        "phaser3",
        "react",
        "vite",
        "typescript"
    ],
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "tsc && vite build --config vite/config.dev.mjs",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "codegen": "graphql-codegen --config codegen.yaml",
        "create-components": "npx @dojoengine/core ../manifests/dev/manifest.json src/dojo/generated/contractComponents.ts http://localhost:5050 0x4d4f3b62432c7a0ff00caa18a6402471b8d153696212ab4a44f127a71233cfa"
    },
    "dependencies": {
        "@dojoengine/core": "^0.7.0-alpha.1",
        "@dojoengine/create-burner": "^0.7.0-alpha.1",
        "@dojoengine/react": "^0.7.0-alpha.1",
        "@dojoengine/recs": "^0.1.35",
        "@dojoengine/state": "^0.7.0-alpha.1",
        "@dojoengine/torii-client": "^0.7.0-alpha.1",
        "@dojoengine/utils": "^0.7.0-alpha.1",
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/fragment-matcher": "^5.0.2",
        "@graphql-codegen/typescript": "^4.0.7",
        "@graphql-codegen/typescript-operations": "^4.2.1",
        "@graphql-codegen/typescript-react-apollo": "^4.3.0",
        "@graphql-codegen/typescript-react-query": "^6.1.0",
        "graphql-request": "^7.0.1",
        "phaser": "^3.80.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rxjs": "^7.8.1",
        "starknet": "^6.1.5",
        "vite-plugin-top-level-await": "^1.3.1",
        "vite-plugin-wasm": "^3.3.0"
    },
    "devDependencies": {
        "@types/node": "^20.4.8",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "terser": "^5.28.1",
        "typescript": "^5.0.3",
        "vite": "^5.2.0"
    }
}
